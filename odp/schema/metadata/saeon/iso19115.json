{
    "$id": "https://odp.saeon.ac.za/schema/metadata/saeon/iso19115",
    "$schema": "https://jschon.dev/ext/translation/schema",
    "type": "object",
    "translations": [
        {
            "t9nScheme": "saeon/datacite-4",
            "t9nTarget": "",
            "t9nConst": {
                "schemaVersion": "http://datacite.org/schema/kernel-4"
            }
        }
    ],
    "properties": {
        "doi": {
            "type": "string",
            "pattern": "^10\\.\\d{4,}(\\.\\d+)*/[-._;()/:a-zA-Z0-9]+$",
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/doi"
                }
            ]
        },
        "title": {
            "type": "string",
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/titles",
                    "t9nArray": {
                        "t9nObject": {
                            "title": {}
                        }
                    }
                }
            ]
        },
        "date": {
            "type": "string",
            "format": "date",
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/publicationYear",
                    "t9nFilter": "date-to-year"
                }
            ]
        },
        "responsibleParties": {
            "type": "array",
            "allOf": [
                {"contains": {"properties": {"role": {"enum": ["originator", "principalInvestigator"]}}}},
                {"contains": {"properties": {"role": {"const": "publisher"}}}},
                {"contains": {"properties": {"role": {"const": "pointOfContact"}}, "required": ["contactInfo"]}}
            ],
            "uniqueItems": true,
            "items": {
                "type": "object",
                "properties": {
                    "individualName": {"type": "string"},
                    "organizationName": {"type": "string"},
                    "positionName": {"type": "string"},
                    "contactInfo": {"type": "string"},
                    "role": {"$ref": "#/$defs/responsiblePartyRole"},
                    "onlineResource": {"$ref": "#/$defs/onlineResource"}
                },
                "additionalProperties": false,
                "required": ["organizationName", "role"],
                "if": {
                    "properties": {
                        "role": {"const": "publisher"}
                    }
                },
                "else": {
                    "$comment": "individualName is required only for a non-publisher role",
                    "required": ["individualName"]
                },
                "translations": [
                    {
                        "t9nScheme": "saeon/datacite-4",
                        "t9nTarget": "/publisher",
                        "t9nCondition": {
                            "properties": {
                                "role": {"const": "publisher"}
                            }
                        },
                        "t9nSource": "0/organizationName"
                    },
                    {
                        "t9nScheme": "saeon/datacite-4",
                        "t9nTarget": "/creators",
                        "t9nCondition": {
                            "properties": {
                                "role": {"enum": ["originator", "principalInvestigator"]}
                            }
                        },
                        "t9nArray": {
                            "t9nObject": {
                                "name": {"t9nSource": "0/individualName"},
                                "nameIdentifiers": {
                                    "t9nArray": {
                                        "t9nObject": {
                                            "nameIdentifier": {"t9nSource": "0/onlineResource/linkage"},
                                            "nameIdentifierScheme": {"t9nSource": "0/onlineResource/linkage", "t9nFilter": "base-url"},
                                            "schemeURI": {"t9nSource": "0/onlineResource/linkage", "t9nFilter": "base-url"}
                                        }
                                    }
                                },
                                "affiliation": {
                                    "t9nArray": {
                                        "t9nObject": {
                                            "affiliation": {"t9nConcat": ["0/organizationName", "0/positionName", "0/contactInfo"], "t9nSep": "\n"}
                                        }
                                    }
                                }
                            }
                        }
                    },
                    {
                        "t9nScheme": "saeon/datacite-4",
                        "t9nTarget": "/contributors",
                        "t9nCondition": {
                            "properties": {
                                "role": {"enum": ["resourceProvider", "custodian", "owner", "distributor", "pointOfContact", "processor"]}
                            }
                        },
                        "t9nArray": {
                            "t9nObject": {
                                "name": {"t9nSource": "0/individualName"},
                                "nameIdentifiers": {
                                    "t9nArray": {
                                        "t9nObject": {
                                            "nameIdentifier": {"t9nSource": "0/onlineResource/linkage"},
                                            "nameIdentifierScheme": {"t9nSource": "0/onlineResource/linkage", "t9nFilter": "base-url"},
                                            "schemeURI": {"t9nSource": "0/onlineResource/linkage", "t9nFilter": "base-url"}
                                        }
                                    }
                                },
                                "affiliation": {
                                    "t9nArray": {
                                        "t9nObject": {
                                            "affiliation": {"t9nConcat": ["0/organizationName", "0/positionName", "0/contactInfo"], "t9nSep": "\n"}
                                        }
                                    }
                                },
                                "contributorType": {
                                    "t9nSource": "0/role",
                                    "t9nFilter": {
                                        "resourceProvider": "HostingInstitution",
                                        "custodian": "DataManager",
                                        "owner": "RightsHolder",
                                        "distributor": "Distributor",
                                        "pointOfContact": "ContactPerson",
                                        "processor": "ProjectMember"
                                    }
                                }
                            }
                        }
                    }
                ]
            }
        },
        "extent": {
            "type": "object",
            "required": ["geographicElements", "temporalElement"],
            "properties": {
                "geographicElements": {
                    "$comment": "Array of geographic elements",
                    "type": "array",
                    "minItems": 1,
                    "uniqueItems": true,
                    "items": {
                        "$comment": "A geographic element",
                        "type": "object",
                        "anyOf": [
                            {"required": ["geographicIdentifier"]},
                            {"required": ["boundingBox"]}
                        ],
                        "properties": {
                            "extentTypeCode": {
                                "$comment": "Indicates whether bounding polygon includes or excludes data: true = inclusion (the default if not specified); false = exclusion",
                                "type": "boolean"
                            },
                            "boundingPolygon": {
                                "$comment": "Bounding polygon: an array of one or more polygons",
                                "type": "array",
                                "minItems": 1,
                                "uniqueItems": true,
                                "items": {
                                    "$comment": "A polygon",
                                    "type": "object",
                                    "required": ["polygon"],
                                    "properties": {
                                        "polygon": {
                                            "$comment": "An array of at least 4 non-aligned points, with the first and last point coinciding",
                                            "type": "array",
                                            "minItems": 4,
                                            "items": {
                                                "$comment": "A geographic point",
                                                "type": "object",
                                                "required": ["longitude", "latitude"],
                                                "properties": {
                                                    "longitude": {"type": "number", "minimum": -180, "maximum": 180},
                                                    "latitude": {"type": "number", "minimum": -90, "maximum": 90}
                                                },
                                                "additionalProperties": false
                                            }
                                        }
                                    },
                                    "additionalProperties": false
                                }
                            },
                            "boundingBox": {
                                "$comment": "Bounding box: approximate geographic position of dataset",
                                "type": "object",
                                "required": ["westBoundLongitude", "eastBoundLongitude", "southBoundLatitude", "northBoundLatitude"],
                                "properties": {
                                    "westBoundLongitude": {"type": "number", "minimum": -180, "maximum": 180},
                                    "eastBoundLongitude": {"type": "number", "minimum": -180, "maximum": 180},
                                    "southBoundLatitude": {"type": "number", "minimum": -90, "maximum": 90},
                                    "northBoundLatitude": {"type": "number", "minimum": -90, "maximum": 90}
                                },
                                "additionalProperties": false
                            },
                            "geographicIdentifier": {"type": "string"}
                        },
                        "additionalProperties": false
                    },
                    "translations": [
                        {
                            "t9nScheme": "saeon/datacite-4",
                            "t9nTarget": "/geoLocations",
                            "t9nArray": {
                                "t9nObject": {
                                    "geoLocationPlace": {"t9nSource": "0/geographicIdentifier"},
                                    "geoLocationBox": {
                                        "t9nObject": {
                                            "westBoundLongitude": {"t9nSource": "0/boundingBox/westBoundLongitude"},
                                            "eastBoundLongitude": {"t9nSource": "0/boundingBox/eastBoundLongitude"},
                                            "southBoundLatitude": {"t9nSource": "0/boundingBox/southBoundLatitude"},
                                            "northBoundLatitude": {"t9nSource": "0/boundingBox/northBoundLatitude"}
                                        }
                                    },
                                    "geoLocationPolygons": {
                                        "t9nSource": "0/boundingPolygon",
                                        "t9nArray": {
                                            "t9nObject": {
                                                "polygonPoints": {
                                                    "t9nSource": "0/polygon",
                                                    "t9nArray": {
                                                        "t9nObject": {
                                                            "pointLongitude": {"t9nSource": "0/longitude"},
                                                            "pointLatitude": {"t9nSource": "0/latitude"}
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    ]
                },
                "verticalElement": {
                    "type": "object",
                    "required": ["minimumValue", "maximumValue", "unitOfMeasure", "verticalDatum"],
                    "properties": {
                        "minimumValue": {
                            "type": "number",
                            "translations": [
                                {
                                    "t9nScheme": "saeon/datacite-4",
                                    "t9nTarget": "/subjects",
                                    "t9nArray": {
                                        "t9nObject": {
                                            "subject": {"t9nCast": "string"},
                                            "subjectScheme": {"t9nConst": "SANS1878 Vertical Extent: minimumValue"}
                                        }
                                    }
                                }
                            ]
                        },
                        "maximumValue": {
                            "type": "number",
                            "translations": [
                                {
                                    "t9nScheme": "saeon/datacite-4",
                                    "t9nTarget": "/subjects",
                                    "t9nArray": {
                                        "t9nObject": {
                                            "subject": {"t9nCast": "string"},
                                            "subjectScheme": {"t9nConst": "SANS1878 Vertical Extent: maximumValue"}
                                        }
                                    }
                                }
                            ]
                        },
                        "unitOfMeasure": {
                            "type": "string",
                            "translations": [
                                {
                                    "t9nScheme": "saeon/datacite-4",
                                    "t9nTarget": "/subjects",
                                    "t9nArray": {
                                        "t9nObject": {
                                            "subject": {},
                                            "subjectScheme": {"t9nConst": "SANS1878 Vertical Extent: unitOfMeasure"}
                                        }
                                    }
                                }
                            ]
                        },
                        "verticalDatum": {
                            "type": "string",
                            "translations": [
                                {
                                    "t9nScheme": "saeon/datacite-4",
                                    "t9nTarget": "/subjects",
                                    "t9nArray": {
                                        "t9nObject": {
                                            "subject": {},
                                            "subjectScheme": {"t9nConst": "SANS1878 Vertical Extent: verticalDatum"}
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "additionalProperties": false
                },
                "temporalElement": {
                    "type": "object",
                    "required": ["startTime", "endTime"],
                    "properties": {
                        "startTime": {"type": "string", "format": "datetime"},
                        "endTime": {"type": "string", "format": "datetime"}
                    },
                    "additionalProperties": false,
                    "translations": [
                        {
                            "t9nScheme": "saeon/datacite-4",
                            "t9nTarget": "/dates",
                            "t9nArray": {
                                "t9nObject": {
                                    "date": {"t9nConcat": ["0/startTime", "0/endTime"], "t9nSep": "/"},
                                    "dateType": {"t9nConst": "Valid"}
                                }
                            }
                        }
                    ]
                }
            },
            "additionalProperties": false
        },
        "languages": {
            "type": "array",
            "items": {"type": "string"},
            "minItems": 1,
            "uniqueItems": true
        },
        "characterSet": {
            "$ref": "#/$defs/characterSetCode"
        },
        "topicCategories": {
            "type": "array",
            "items": {"$ref": "#/$defs/topicCategoryCode"},
            "minItems": 1,
            "uniqueItems": true,
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/subjects",
                    "t9nArray": {
                        "t9nObject": {
                            "subject": {},
                            "subjectScheme": {"t9nConst": "SANS1878 Dataset Topic Category"}
                        }
                    }
                }
            ]
        },
        "spatialResolution": {
            "type": "string"
        },
        "abstract": {
            "type": "string",
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/descriptions",
                    "t9nArray": {
                        "t9nObject": {
                            "description": {},
                            "descriptionType": {"t9nConst": "Abstract"}
                        }
                    }
                }
            ]
        },
        "distributionFormats": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "formatName": {"type": "string"},
                    "formatVersion": {"type": "string"}
                },
                "required": ["formatName"],
                "additionalProperties": false
            },
            "minItems": 1,
            "uniqueItems": true,
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/formats",
                    "t9nArray": {"t9nConcat": ["0/formatName", "0/formatVersion"], "t9nSep": " "}
                }
            ]
        },
        "spatialRepresentationTypes": {
            "type": "array",
            "items": {"$ref": "#/$defs/spatialRepresentationTypeCode"},
            "minItems": 1,
            "uniqueItems": true,
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/subjects",
                    "t9nArray": {
                        "t9nObject": {
                            "subject": {},
                            "subjectScheme": {"t9nConst": "SANS1878 Spatial Representation Type"}
                        }
                    }
                }
            ]
        },
        "referenceSystemName": {
            "type": "object",
            "properties": {
                "codeSpace": {"type": "string"},
                "version": {"type": "string"}
            },
            "required": ["codeSpace"],
            "additionalProperties": false,
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/subjects",
                    "t9nArray": {
                        "t9nObject": {
                            "subject": {"t9nConcat": ["0/codeSpace", "0/version"], "t9nSep": " "},
                            "subjectScheme": {"t9nConst": "SANS1878 Reference System"}
                        }
                    }
                }
            ]
        },
        "lineageStatement": {
            "type": "string",
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/descriptions",
                    "t9nArray": {
                        "t9nObject": {
                            "description": {},
                            "descriptionType": {"t9nConst": "Methods"}
                        }
                    }
                }
            ]
        },
        "onlineResources": {
            "type": "array",
            "items": {
                "$ref": "#/$defs/onlineResource",
                "required": ["description"],
                "translations": [
                    {
                        "$comment": "case 1: Download link with an (optional) application profile specifying a single file type",
                        "t9nScheme": "saeon/datacite-4",
                        "t9nTarget": "/immutableResource",
                        "t9nCondition": {
                            "properties": {
                                "description": {"const": "download"},
                                "applicationProfile": {"pattern": "^\\w+$"}
                            }
                        },
                        "t9nObject": {
                            "resourceName": {"t9nSource": "0/name"},
                            "resourceDownload": {
                                "t9nObject": {
                                    "downloadURL": {"t9nSource": "0/linkage"},
                                    "fileFormat": {"t9nSource": "0/applicationProfile"}
                                }
                            }
                        }
                    },
                    {
                        "$comment": "case 2: Download link with an application profile that specifies a list of file types, implying an archive",
                        "t9nScheme": "saeon/datacite-4",
                        "t9nTarget": "/immutableResource",
                        "t9nCondition": {
                            "properties": {
                                "description": {"const": "download"},
                                "applicationProfile": {"pattern": "^\\[\\s*\\w+\\s*(,\\s*\\w+\\s*)*]$"}
                            },
                            "required": ["applicationProfile"]
                        },
                        "t9nObject": {
                            "resourceName": {"t9nSource": "0/name"},
                            "resourceDownload": {
                                "t9nObject": {
                                    "downloadURL": {"t9nSource": "0/linkage"},
                                    "archive": {"t9nConst": true},
                                    "archivedFormats": {"t9nSource": "0/applicationProfile", "t9nFilter": "split-archived-formats"}
                                }
                            }
                        }
                    },
                    {
                        "$comment": "case 3: Query link with an (optional) application profile constrained to the serverType vocabulary",
                        "t9nScheme": "saeon/datacite-4",
                        "t9nTarget": "/linkedResources",
                        "t9nCondition": {
                            "properties": {
                                "description": {"const": "search"},
                                "applicationProfile": {"enum": ["GeoServer", "Esri", "THREDDS"]}
                            }
                        },
                        "t9nArray": {
                            "t9nObject": {
                                "resourceURL": {"t9nSource": "0/linkage"},
                                "resourceName": {"t9nSource": "0/name"},
                                "linkedResourceType": {"t9nConst": "Query"},
                                "serverType": {"t9nSource": "0/applicationProfile"}
                            }
                        }
                    },
                    {
                        "$comment": "case 4: Other supported types of linked resource",
                        "t9nScheme": "saeon/datacite-4",
                        "t9nTarget": "/linkedResources",
                        "t9nCondition": {
                            "properties": {
                                "description": {"enum": ["information", "offlineAccess", "order"]}
                            }
                        },
                        "t9nArray": {
                            "t9nObject": {
                                "resourceURL": {"t9nSource": "0/linkage"},
                                "resourceName": {"t9nSource": "0/name"},
                                "linkedResourceType": {
                                    "t9nSource": "0/description",
                                    "t9nFilter": {
                                        "information": "Information",
                                        "offlineAccess": "OfflineAccess",
                                        "order": "ConditionalAccess"
                                    }
                                }
                            }
                        }
                    }
                ]
            },
            "minItems": 1,
            "uniqueItems": true
        },
        "fileIdentifier": {
            "type": "string",
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/identifiers",
                    "t9nArray": {
                        "t9nObject": {
                            "identifier": {},
                            "identifierType": {"t9nConst": "AccessionID"}
                        }
                    }
                }
            ]
        },
