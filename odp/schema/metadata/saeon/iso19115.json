            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/subjects",
                    "t9nArray": {
                        "t9nObject": {
                            "subject": {},
                            "subjectScheme": {"t9nConst": "SANS1878 Status"}
                        }
                    }
                }
            ]
        },
        "descriptiveKeywords": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "keyword": {"type": "string"},
                    "keywordType": {"$ref": "#/$defs/keywordType"}
                },
                "required": ["keyword", "keywordType"],
                "additionalProperties": false
            },
            "uniqueItems": true,
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/subjects",
                    "t9nArray": {
                        "t9nObject": {
                            "subject": {"t9nSource": "0/keyword"},
                            "subjectScheme": {
                                "t9nSource": "0/keywordType",
                                "t9nFilter": {
                                    "discipline": "SANS1878 keywordType discipline",
                                    "place": "SANS1878 keywordType place",
                                    "stratum": "SANS1878 keywordType stratum",
                                    "temporal": "SANS1878 keywordType temporal",
                                    "theme": "SANS1878 keywordType theme",
                                    "general": "SANS1878 keywordType general"
                                }
                            }
                        }
