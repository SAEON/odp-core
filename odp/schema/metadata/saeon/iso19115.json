            "type": "string",
            "const": "ISO19115"
        },
        "metadataStandardVersion": {
            "type": "string",
            "const": "ISO19115 - SAEON Profile"
        },
        "metadataLanguage": {
            "type": "string"
        },
        "metadataCharacterSet": {
            "$ref": "#/$defs/characterSetCode"
        },
