                            }
                        ]
                    },
                    "useConstraints": {
                        "type": "array",
                        "items": {"$ref": "#/$defs/restrictionCode"},
                        "uniqueItems": true,
                        "translations": [
                            {
                                "t9nScheme": "saeon/datacite-4",
                                "t9nTarget": "/subjects",
                                "t9nArray": {
                                    "t9nObject": {
                                        "subject": {},
                                        "subjectScheme": {"t9nConst": "SANS1878 Constraint"}
                                    }
                                }
                            }
                        ]
                    },
                    "classification": {
                        "$ref": "#/$defs/classificationCode",
                        "translations": [
                            {
                                "t9nScheme": "saeon/datacite-4",
                                "t9nTarget": "/subjects",
                                "t9nArray": {
                                    "t9nObject": {
                                        "subject": {},
                                        "subjectScheme": {"t9nConst": "SANS1878 Security Classification"}
                                    }
                                }
                            }
                        ]
