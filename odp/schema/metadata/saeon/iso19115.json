                    "t9nArray": {
                        "t9nObject": {
                            "date": {},
                            "dateType": {"t9nConst": "Issued"}
                        }
                    }
                }
            ]
        },
        "purpose": {
            "type": "string",
            "translations": [
                {
                    "t9nScheme": "saeon/datacite-4",
                    "t9nTarget": "/descriptions",
                    "t9nArray": {
                        "t9nObject": {
                            "description": {},
                            "descriptionType": {"t9nConst": "Other"}
