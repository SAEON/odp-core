            "items": {
                "type": "object",
                "properties": {
                    "useLimitations": {
                        "type": "array",
                        "items": {"type": "string"},
                        "uniqueItems": true,
                        "translations": [
                            {
                                "t9nScheme": "saeon/datacite-4",
